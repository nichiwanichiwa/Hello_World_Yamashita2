<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.yamashitamasaki.hello_world.Hijousyoku">


    <Button
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="ホーム"
        android:id="@+id/homebutton3"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="非常食"
        android:id="@+id/hijousyoku4"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/homebutton3" />

    <Button
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="備蓄品"
        android:id="@+id/bichiku4"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/hijousyoku4" />

    <Button
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:text="設定"
        android:id="@+id/select4"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/bichiku4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/retoruto_gohan"
        android:src="@drawable/retoruto_gohan"
        android:layout_below="@+id/textView21"
        android:layout_alignEnd="@+id/homebutton3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/kandume"
        android:layout_alignTop="@+id/retoruto_gohan"
        android:layout_toEndOf="@+id/homebutton3"
        android:src="@drawable/kandume" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/kanmen"
        android:src="@drawable/kanmen"
        android:layout_alignTop="@+id/kandume"
        android:layout_toEndOf="@+id/kandume" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/kanpan"
        android:src="@drawable/kanpan"
        android:layout_alignTop="@+id/kanmen"
        android:layout_toEndOf="@+id/kanmen"
        android:scaleType="fitXY" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/kandume2"
        android:src="@drawable/kandume"
        android:layout_below="@+id/textView22"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="主食"
        android:id="@+id/textView21"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/retoruto"
        android:layout_alignTop="@+id/kandume2"
        android:layout_toEndOf="@+id/kandume2"
        android:src="@drawable/retoruto" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/furizu_dorai"
        android:layout_alignTop="@+id/retoruto"
        android:layout_toEndOf="@+id/retoruto"
        android:src="@drawable/furizu_dorai" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="主菜"
        android:id="@+id/textView22"
        android:layout_below="@+id/retoruto_gohan"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="その他"
        android:id="@+id/textView23"
        android:layout_below="@+id/kandume2"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:id="@+id/mizu"
        android:src="@drawable/mizu"
        android:layout_below="@+id/textView23"
        android:layout_alignParentStart="true"
        android:scaleType="fitXY" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pokari_hunmatu"
        android:layout_alignTop="@+id/mizu"
        android:layout_toEndOf="@+id/mizu"
        android:src="@drawable/pokari_hunmatu" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/karori_meito"
        android:layout_alignTop="@+id/pokari_hunmatu"
        android:layout_toEndOf="@+id/pokari_hunmatu"
        android:src="@drawable/karori_meito" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/okasi"
        android:layout_alignTop="@+id/karori_meito"
        android:layout_toEndOf="@+id/karori_meito"
        android:src="@drawable/okasi"
        android:scaleType="fitXY" />
</RelativeLayout>
